Button4.Position = UDim2.new(0, 10, 0, 240)
Button4.Size = UDim2.new(0, 250, 0, 50)
Button4.Text = "Infinite Jump"

-- Setting up Status Label (for Infinite Jump)
StatusLabel.Parent = MainFrame
