local ScreenGui = Instance.new("ScreenGui")
local MainFrame = Instance.new("Frame")
local TitleLabel = Instance.new("TextLabel")
local CloseButton = Instance.new("TextButton")
local Icon = Instance.new("ImageLabel")
local Button1 = Instance.new("TextButton")
local Button2 = Instance.new("TextButton")
local Button3 = Instance.new("TextButton")
local Button4 = Instance.new("TextButton")
local StatusLabel = Instance.new("TextLabel")
local SpeedLabel = Instance.new("TextLabel")
local SpeedPlusButton = Instance.new("TextButton")
local SpeedMinusButton = Instance.new("TextButton")
local TeleportButton = Instance.new("TextButton")
local AdminLabel = Instance.new("TextLabel")
local StartButton = Instance.new("TextButton")

-- Setting up ScreenGui
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.Name = "AllMenuGui"

-- Setting up MainFrame
MainFrame.Parent = ScreenGui
MainFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)  -- Dark gray background
MainFrame.Position = UDim2.new(0.5, -250, 0.5, -200)  -- Center the frame
MainFrame.Size = UDim2.new(0, 550, 0, 450)
MainFrame.BorderSizePixel = 2
MainFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)  -- Black border

-- Setting up TitleLabel
TitleLabel.Parent = MainFrame
TitleLabel.BackgroundTransparency = 1
TitleLabel.Size = UDim2.new(1, 0, 0, 50)
TitleLabel.Text = "All Menu | @ NazamOfficial"
TitleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)  -- White text
TitleLabel.TextSize = 24
TitleLabel.TextXAlignment = Enum.TextXAlignment.Left
TitleLabel.Position = UDim2.new(0, 55, 0, 0)

-- Setting up CloseButton
CloseButton.Parent = MainFrame
CloseButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)  -- Red
CloseButton.Position = UDim2.new(1, -40, 0, 10)
CloseButton.Size = UDim2.new(0, 30, 0, 30)
CloseButton.Text = "X"
CloseButton.TextColor3 = Color3.fromRGB(255, 255, 255)  -- White text
CloseButton.TextSize = 18

-- Setting up Icon
Icon.Parent = MainFrame
Icon.Size = UDim2.new(0, 40, 0, 40)
Icon.Position = UDim2.new(0, 10, 0, 5)
Icon.Image = "rbxassetid://12345678"  -- Ganti dengan ID asset gambar sesuai keinginan

-- Setting up Buttons (Fly, Spec + Tp, Cek Own/Admin, Infinite Jump)
Button1.Parent = MainFrame
Button1.BackgroundColor3 = Color3.fromRGB(51, 51, 51)  -- Dark gray button
Button1.Position = UDim2.new(0, 10, 0, 60)
Button1.Size = UDim2.new(0, 250, 0, 50)
Button1.Text = "Fly"

Button2.Parent = MainFrame
